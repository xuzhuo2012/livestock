<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:tools="http://schemas.android.com/tools"
                android:id="@+id/RelativeLayout1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center_horizontal"
                android:orientation="vertical"
                android:paddingBottom="@dimen/activity_vertical_margin"
                android:paddingLeft="@dimen/activity_horizontal_margin"
                android:paddingRight="@dimen/activity_horizontal_margin"
                android:paddingTop="@dimen/activity_vertical_margin"
                tools:context=".AddFeedActivity">
                
    
        <Button
            android:id="@+id/add_feed_submit"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:text="@string/action_send"/>
      
        
        <TextView
            android:id="@+id/add_feed_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/add_feed"
          	android:layout_centerHorizontal="true"
           android:layout_marginTop="10dp"
            android:textAppearance="?android:attr/textAppearanceLarge"/>
        
        <Button
            android:id="@+id/add_feed_cancel_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:text="@string/add_cancel"/>   
        
    

    <TextView
            android:id="@+id/add_feed_name_label"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/add_feed_submit"
            android:layout_marginTop="26dp"
            android:text="@string/feed_changeDate"/>

    

    <EditText
            android:id="@+id/add_feed_changeDate"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/add_feed_name_label"
            android:layout_alignBottom="@+id/add_feed_name_label"
            android:layout_alignLeft="@+id/add_feed_name"
            android:layout_alignParentRight="true"
            android:ems="10"
            android:inputType="datetime">

        <requestFocus/>
    </EditText>

    <TextView
            android:id="@+id/add_feed_factory_label"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/add_feed_changeDate_label"
            android:layout_below="@+id/add_feed_name"
            android:layout_marginTop="13dp"
            android:text="@string/feed_factory"/>

    <EditText
            android:id="@+id/add_feed_factory"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/add_feed_name"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/add_feed_name"
            android:ems="10"
            android:inputType="none"/>

    <EditText
            android:id="@+id/add_feed_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/add_feed_changeDate"
            android:layout_toRightOf="@+id/add_feed_changeDate_label"
            android:ems="10"
            android:inputType="none">
    </EditText>

    <TextView
            android:id="@+id/add_feed_changeDate_label"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/add_feed_name"
            android:layout_alignBottom="@+id/add_feed_name"
            android:layout_alignLeft="@+id/add_feed_name_label"
            android:text="@string/feed_feedName"/>

    <TextView
            android:id="@+id/add_feed_changeType_label"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/add_feed_batchNumber_label"
            android:layout_below="@+id/add_feed_batchNumber"
            android:layout_marginTop="15dp"
            android:text="@string/feed_changeType"/>

    <EditText
            android:id="@+id/add_feed_changeType"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/add_feed_batchNumber"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/add_feed_batchNumber"
            android:ems="10"
            android:inputType="none"/>

    <TextView
            android:id="@+id/add_feed_changeCount_label"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignRight="@+id/add_feed_changeType_label"
            android:layout_below="@+id/add_feed_changeType"
            android:layout_marginTop="12dp"
            android:text="@string/feed_changeCount"/>

    <EditText
            android:id="@+id/add_feed_changeCount"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/add_feed_changeCount_label"
            android:layout_alignBottom="@+id/add_feed_changeCount_label"
            android:layout_alignLeft="@+id/add_feed_changeType"
            android:layout_alignParentRight="true"
            android:ems="10"
            android:inputType="number"/>

    <EditText
            android:id="@+id/add_feed_batchNumber"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/add_feed_factory"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/add_feed_factory_label"
            android:ems="10"
            android:inputType="none"/>

    <TextView
            android:id="@+id/add_feed_batchNumber_label"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/add_feed_batchNumber"
            android:layout_alignBottom="@+id/add_feed_batchNumber"
            android:layout_toLeftOf="@+id/add_feed_batchNumber"
            android:text="@string/feed_batchNumber"/>

    <EditText
            android:id="@+id/add_feed_remark"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/add_feed_remark_label"
            android:layout_alignBottom="@+id/add_feed_remark_label"
            android:layout_alignLeft="@+id/add_feed_changeCount"
            android:layout_alignParentRight="true"
            android:ems="10"
            android:inputType="none"/>

    <TextView
            android:id="@+id/add_feed_remark_label"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/add_feed_changeCount_label"
            android:layout_below="@+id/add_feed_changeCount"
            android:layout_marginTop="15dp"
            android:text="@string/feed_remark"/>

    

   

</RelativeLayout>